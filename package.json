{
  "name": "@geekist/llm-core",
  "version": "1.21.0",
  "license": "Apache-2.0",
  "description": "A compact, vendor-neutral runtime for building and shipping LLM workflows (agents, RAG, ingest, HITL) with deterministic structure, strong typing, and test-first semantics.",
  "keywords": [
    "typescript",
    "javascript",
    "llm",
    "ai",
    "workflow",
    "pipeline",
    "pipelines",
    "rag",
    "agents",
    "tools",
    "adapters",
    "langchain",
    "llamaindex",
    "ai-sdk",
    "geekist",
    "dag",
    "openai",
    "ollama"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/theGeekist/llm-core.git"
  },
  "homepage": "https://llm-core.geekist.co/",
  "bugs": {
    "url": "https://github.com/theGeekist/llm-core/issues"
  },
  "author": "Geekist <hello@geekist.co>",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@wpkernel/pipeline": "1.1.0"
  },
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "type": "module",
  "private": false,
  "scripts": {
    "build": "bun run build:esm && bun run build:cjs && bun run build:types",
    "build:esm": "bun build ./index.ts ./src/workflow/index.ts ./src/adapters/index.ts ./src/adapters/*/index.ts ./src/interaction/index.ts ./src/shared/diagnostics.ts ./src/shared/trace.ts --outdir dist/esm --root . --sourcemap=external --splitting --format=esm --target=browser",
    "build:cjs": "bun build ./index.ts ./src/workflow/index.ts ./src/adapters/index.ts ./src/adapters/*/index.ts ./src/interaction/index.ts ./src/shared/diagnostics.ts ./src/shared/trace.ts --outdir dist/cjs --root . --sourcemap=external --format=cjs --target=node --entry-naming=[dir]/[name].cjs --chunk-naming=[name]-[hash].cjs",
    "build:types": "tsc -p tsconfig.build.json --emitDeclarationOnly",
    "prepare": "git config core.hooksPath .githooks",
    "format": "prettier \"**/*.{ts,tsx,js,jsx,json,md}\" --write",
    "format:check": "prettier \"**/*.{ts,tsx,js,jsx,json,md}\" --check",
    "docs:format": "prettier \"docs/**/*.md\" --write",
    "docs:format:check": "prettier \"docs/**/*.md\" --check",
    "docs:dev": "bun node_modules/vitepress/dist/node/cli.js dev docs",
    "docs:build": "bun node_modules/vitepress/dist/node/cli.js build docs",
    "docs:preview": "bun node_modules/vitepress/dist/node/cli.js preview docs",
    "docs:snippets:typecheck": "bun tsc -p docs/snippets/tsconfig.json --noEmit",
    "lint": "eslint \"**/*.{ts,tsx,js,jsx}\"",
    "lint:fix": "eslint \"**/*.{ts,tsx,js,jsx}\" --fix",
    "release:check": "bun run lint && bun run typecheck && bun run test",
    "release:build": "bun run release:check && bun run build",
    "publish:docs": "bun run docs:build",
    "publish:npm": "bun run build && bun publish",
    "publish:all": "bun run publish:npm && bun run publish:docs",
    "test": "bun test",
    "test:coverage": "bun test --coverage --coverage-reporter=text",
    "typecheck": "tsc --noEmit",
    "typecheck:tests": "tsc -p tsconfig.test.json --noEmit"
  },
  "devDependencies": {
    "@ai-sdk-tools/cache": "^1.2.0",
    "@ai-sdk-tools/memory": "^1.2.0",
    "@ai-sdk-tools/store": "^1.2.0",
    "@ai-sdk/anthropic": "^2.0.56",
    "@ai-sdk/langchain": "^2.0.3",
    "@ai-sdk/openai": "^2.0.88",
    "@ai-sdk/provider": "^3.0.0",
    "@ai-sdk/provider-utils": "^4.0.1",
    "@ai-sdk/react": "^3.0.15",
    "@assistant-ui/react": "^0.11.53",
    "@assistant-ui/react-ai-sdk": "^1.1.20",
    "@langchain/community": "^1.1.1",
    "@langchain/core": "^1.1.8",
    "@langchain/langgraph": "^1.0.7",
    "@langchain/langgraph-checkpoint": "^1.0.0",
    "@langchain/ollama": "^1.1.0",
    "@langchain/openai": "^1.2.0",
    "@langchain/textsplitters": "^1.0.1",
    "@llamaindex/openai": "^0.4.22",
    "@llamaindex/workflow": "^1.1.24",
    "@llamaindex/workflow-core": "^1.3.3",
    "@openai/chatkit": "^1.2.0",
    "@types/bun": "latest",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "ai": "^5.0.116",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-sonarjs": "^3.0.5",
    "langchain": "^1.2.3",
    "llamaindex": "^0.12.1",
    "markdown-it-mermaid": "^0.2.5",
    "mermaid": "^11.12.2",
    "ollama": "^0.6.3",
    "ollama-ai-provider-v2": "^1.5.5",
    "openai": "^6.15.0",
    "prettier": "3.3.3",
    "vitepress": "^1.6.4",
    "vitepress-plugin-mermaid": "^2.0.17",
    "vitepress-plugin-tabs": "^0.7.3",
    "zod": "^4.2.1",
    "zod-to-json-schema": "^3.25.0"
  },
  "imports": {
    "#workflow": "./src/workflow/index.ts",
    "#workflow/*": "./src/workflow/*.ts",
    "#adapters": "./src/adapters/index.ts",
    "#adapters/*": "./src/adapters/*/index.ts",
    "#recipes": "./src/recipes/index.ts",
    "#interaction": "./src/interaction/index.ts"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "browser": "./dist/esm/index.js",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/esm/index.js"
    },
